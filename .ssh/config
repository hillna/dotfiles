# Olark Hosts

Host git.olark.net
	HostName git.olark.net
	User gitosis
	PreferredAuthentications publickey
	IdentityFile ~/.ssh/id_rsa

# Personal Hosts

Host github
	HostName github.com
	User git
	IdentityFile ~/.ssh/id_rsa

Host whatbox
	HostName chocolate.whatbox.ca
	User reign
	IdentityFile ~/.ssh/id_rsa

Host *
	ForwardAgent yes

# Global Options
ServerAliveInterval 30
#ServerAliveCountMax 120
